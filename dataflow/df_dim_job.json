{
	"name": "df_dim_job",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "title_principal",
						"type": "DatasetReference"
					},
					"name": "jobs"
				},
				{
					"dataset": {
						"referenceName": "dim_job",
						"type": "DatasetReference"
					},
					"name": "JobDimMaxID"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dim_job",
						"type": "DatasetReference"
					},
					"name": "sinkJob"
				}
			],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     job_id_job as string (\"10\")",
				"}",
				"source(output(",
				"          JOB as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     query: 'SELECT DISTINCT JOB\\nFROM RAW_STAGE_SCHEMA.TITLE_PRINCIPALS',",
				"     format: 'query') ~> jobs",
				"source(output(",
				"          MAX_JOB_SK as decimal(38,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     query: 'SELECT NVL(MAX(JOB_SK), 0) as\\nMAX_JOB_SK FROM \\nIMDB_SCHEMA.DIM_JOB',",
				"     format: 'query') ~> JobDimMaxID",
				"jobs, JobDimMaxID join(1==1,",
				"     joinType:'cross',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 keyGenerate(output(RowNum as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 derive(JOB_SK = MAX_JOB_SK + RowNum,",
				"          DI_JOB_ID = uuid(),",
				"          DI_LOAD_DT = currentTimestamp()) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          JOB_SK as decimal(38,0),",
				"          JOB_NAME as string,",
				"          DI_JOB_ID as string,",
				"          DI_LOAD_DT as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          JOB_SK,",
				"          JOB_NAME = JOB,",
				"          DI_JOB_ID,",
				"          DI_LOAD_DT",
				"     )) ~> sinkJob"
			]
		}
	}
}