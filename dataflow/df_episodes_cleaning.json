{
	"name": "df_episodes",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_episodes",
						"type": "DatasetReference"
					},
					"name": "source"

				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_episodes_silver",
						"type": "DatasetReference"
					},
					"name": "sink"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn"
				}
			],
			"scriptLines": [
				"source(output(",
				"          tconst as string,",
				"          parentTconst as string,",
				"          seasonNumber as string,",
				"          episodeNumber as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source",
				"source derive(seasonNumber = toInteger(iif(seasonNumber == '\\\\N', '-1', seasonNumber)),",
				"          episodeNumber = toInteger(iif(episodeNumber == '\\\\N', '-1', episodeNumber))) ~> derivedColumn",
				"derivedColumn sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink"
			]
		}
	}
}